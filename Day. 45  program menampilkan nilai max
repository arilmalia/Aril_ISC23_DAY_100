package ISC_2023;
public class Main {
    
    public static void main(String[] args) {
        
        
        int[] a = {12, 21, 36, 18, 26};
        int jumlah = a.length;
        int max = a[0];
        
        for(int i = 0 ; i < jumlah; i++){	
            System.out.print(a[i]+" ");
            if(a[i] > max){
                max = a[i];
            }
            
        }System.out.print("");
        System.out.print("max = "+max);
        
    }
    
}
