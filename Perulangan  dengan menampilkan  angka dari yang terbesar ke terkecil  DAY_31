package ISC_2023;
import java.util.Scanner;
public class Main {
    
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Masukan angka : ");
        int a = input.nextInt();

        while(1<=a){
            System.out.println(" "+a);
            a--;
            
            
        }
        
        
        
    }
}
