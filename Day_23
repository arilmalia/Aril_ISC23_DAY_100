package isc_2023;
import java.util.Scanner;
public class Main {
    
    public static void main(String[] args) {
       Scanner input = new Scanner (System.in);
        System.out.print("Masukan  batas : ");
        int batas = input.nextInt();
        for (int a = 3; a<=batas; a+=3){
            System.out.print(a+ " "); 
        }
    }
}
