package ISC_2023;
public class Main {
    
    public static void main(String[] args) {
        
        int a [] = {10, 20, 30};
        int b = 0;
        for (int i = 0; i <a.length; i++){
        b += a[i];
        }
        for (int i = 0 ; i< a.length; i++){
            System.out.println("index ke - "+i+ " :"+ a[i] +" ");
        }
        System.out.println("");
        System.out.print("hasil penjumlahan index : " + b);
    }
}
