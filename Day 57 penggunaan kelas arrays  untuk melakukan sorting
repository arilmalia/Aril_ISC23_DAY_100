package ISC_2023;
import java.util.Arrays;
public class Main {
    
    public static void main(String[] args) {
        
        double  [] koma = {3.9, 0.1,1.6, 9.9, 4.5};
        System.out.println("Seblm di Sorting   : " +Arrays.toString(koma));
        
        System.out.println("-------------------------------------------");
        
        Arrays.sort(koma);
        System.out.print("Sesudah di Sorting : "+Arrays.toString(koma));

    }
}
