package day15;
import java.util.Scanner;
public class Main {
    
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int a, b, c, d;
        
        System.out.print("masukan gaji poko\t : ");
        a = input.nextInt();
        b = 55000;
        
        System.out.print("masukan jumlah lembur\t : ");
        c = input.nextInt();
        
        d = (c*b)+a; 
        System.out.print("gaji bersi anda adalah\t : Rp "+d);
    }
    
}
