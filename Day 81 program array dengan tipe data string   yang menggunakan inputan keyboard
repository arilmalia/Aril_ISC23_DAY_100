package ISC_23;
import java.util.*;
public class Main {
    
    public static void main(String[] args) {
        Scanner input = new Scanner (System.in);
        
        // Buat array dengan tipe data string
        String[] ril = new String[5];

        // Baca inputan keyboard
        for (int i = 0; i < ril.length; i++) {
            System.out.print("Masukan nama : ");
            ril[i] = input.nextLine();
        }
            for (int j = 0 ; j < ril.length; j++){
            System.out.print(ril[j]+" , ");
            } 
        
    }
}


